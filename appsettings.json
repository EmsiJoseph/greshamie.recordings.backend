{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedOrigins": "*",
  "ConnectionStrings": {
    "LocalDefaultConnection": "Server=(LocalDB)\\Local;Database=GreshamRecordings;Trusted_Connection=True;",
    "AzureBlobStorage": ""
  },
  "ReverseProxy": {
    "Routes": {
      "nextjs": {
        "ClusterId": "nextjsCluster",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "nextjsCluster": {
        "Destinations": {
          "nextjs": {
            "Address": "http://frontend:3000/"
          }
        }
      }
    }
  }
}
